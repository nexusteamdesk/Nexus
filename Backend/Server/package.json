{
  "name": "nexus_backend",
  "version": "2.0.0",
  "description": "Nexus AI Backend with Queue System",
  "main": "gemini_server.js",
  "scripts": {
    "start": "node gemini_server.js",
    "worker": "node worker.js",
    "monitor": "node monitoring.js",
    "backup": "node backup.js",
    "dev": "nodemon gemini_server.js",
    "dev:worker": "nodemon worker.js",
    "dev:all": "concurrently -n \"API,Worker,Monitor\" -c \"cyan,green,yellow\" \"npm run dev\" \"npm run dev:worker\" \"npm run monitor\"",
    "start:all": "concurrently -n \"API,Worker\" -c \"cyan,green\" \"npm start\" \"npm run worker\"",
    "start:full": "concurrently -n \"API,Worker,Monitor\" -c \"cyan,green,yellow\" \"npm start\" \"npm run worker\" \"npm run monitor\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.75.0",
    "bullmq": "^5.4.0",
    "cheerio": "^1.1.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "keyword-extractor": "^0.0.28",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "nodemon": "^3.1.10",
    "sentiment": "^5.0.2"
  }
}
