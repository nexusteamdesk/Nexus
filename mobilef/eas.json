{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.21.0",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "22.21.0",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "22.21.0",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EXPO_USE_SOURCE": "1"
      }
    }
  },
  "submit": {
    "production": {}
  },
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "local"
  }
}
